@model IEnumerable<Camada.Dominio.Mensagens.GridMoney_Count>

@{
    ViewBag.Title = "Create";
}

<h2><i class="fa fa-list"></i> Details</h2>
<div class="form-horizontal">
    <h4>Calculator</h4>
    <hr />
</div>

