        <th>
            @Html.DisplayNameFor(model => model.identificadorUnidadeGestora)
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.numeroAto)
