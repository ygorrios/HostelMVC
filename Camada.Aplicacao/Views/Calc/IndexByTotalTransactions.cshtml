            @Html.Label("Total Calculator", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-2">
                @Html.Editor("TotalCalc", new
           {
               htmlAttributes = new
               {
                   @class = "form-control",
                   @readonly = "readonly"
               }
           })
            </div>
        </div>
        <hr />

