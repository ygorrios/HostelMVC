@model IEnumerable<Camada.Dominio.Entidades.AP_APE>

@{
    ViewBag.Title = "Teste";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Página Inicial</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
